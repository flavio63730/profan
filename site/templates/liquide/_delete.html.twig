<form method="post" action="{{ path('app_liquide_delete', {'id': liquide.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ liquide.id) }}">
    <button class="btn btn-danger">
        <i class="fa fa-trash"></i>
    </button>
</form>
